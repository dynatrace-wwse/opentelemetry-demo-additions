{
  "entryPointRule": {
    "ruleName": "Astroshop-OTEL-OA-demo",
    "ruleAction": "CREATE_ENTRYPOINT",
    "matchers": [
      {
        "source": "ATTRIBUTE",
        "sourceKey": "next.span_type",
        "type": "EQUALS",
        "value": "Node.runHandler",
        "caseSensitive": false
      },
      {
        "source": "SPAN_KIND",
        "type": "EQUALS",
        "spanKindValue": "INTERNAL"
      }
    ]
  }
}