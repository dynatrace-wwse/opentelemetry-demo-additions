{
  "contextPropagationRule": {
    "ruleName": "dynatrace-aws-appobs-demo-sqs nodejs",
    "ruleAction": "PROPAGATE",
    "matchers": [
      {
        "source": "INSTRUMENTATION_SCOPE_NAME",
        "type": "EQUALS",
        "value": "@opentelemetry/instrumentation-aws-sdk",
        "caseSensitive": true
      },
      {
        "source": "SPAN_NAME",
        "type": "CONTAINS",
        "value": "opentelemetry",
        "caseSensitive": false
      }
    ]
  }
}